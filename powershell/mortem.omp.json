{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "green": "#45F1C2",
    "red": "#CD4277",
    "magenta": "#BC93FF",
    "turquoise": "#14A5AE",
    "blue": "#0CA0D8",
    "orange": "#CD5E42",
    "dark_blue": "#007ACC",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue_frappe": "#89B4FA"
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:green",
          "properties": {
            "time_format": "01/02 03:04PM"
          },
          "style": "diamond",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "p:blue",
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": " \ueaf7 {{ path .Path .Location }} ",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_upstream_icon": true,
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_worktree_count": true
          },
          "style": "plain",
          "template": "{{ if or (.Working.Changed) (.Staging.Changed) (gt .StashCount 0) }}<p:magenta>{{ .UpstreamIcon }}{{ .HEAD }}</>{{ else }}<p:green>{{ .UpstreamIcon }}{{ .HEAD }}</>{{ end }}{{ if (gt .Ahead 0)}}<p:turquoise> {{ .BranchStatus }}</>{{ end }}{{ if (gt .Behind 0)}}<p:turquoise> {{ .BranchStatus }}</>{{ end }}{{ if .Staging.Changed }} <p:green>{{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }} <p:red>{{ .Working.String }}</>{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:orange",
          "style": "plain",
          "template": "\ue3bf ",
          "type": "root"
        },
        {
          "foreground": "p:dark_blue",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
